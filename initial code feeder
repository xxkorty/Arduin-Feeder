

void loop() {

//start spreader  
analogWrite(6,255);


//start solenoid
analogWrite(5,255);
//delay solenoid retract
delay(1000);
//stop solenoid
analogWrite(5,0);
delay(3000);

//second start of solenoid
analogWrite(5,255);
delay(1000);
//stop second start of solenoid
analogWrite(5,0);
delay(3000);


//stop spreader
analogWrite(6,0);
//wait 12 hours
delay(200000);

  // put your main code here, to run repeatedly:

}
